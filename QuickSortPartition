public class Partition {


public static int partition(int[] input, int index) {
    input = swap(0, index, input);
    int i = 0;
    int j = input.length - 1;
    
    while(i <= j) {
        
      while(input[i] <= input[0]) {
        i++;
      }

      while(input[j] >= input[0]) {
        j--;
      }
    input = swap(i, j, input);
    }
    
    return j;
  }
  }
}
